class Solution {
    public int rob(int[] nums) {
        int dp[]=new int[nums.length];
        Arrays.fill(dp,-1);
        return func(nums,0,dp);

        
    }
    public int func(int nums[],int idx,int dp[]){
        if(idx>=nums.length){
            return 0;
        }
        if(dp[idx]!=-1){
            return dp[idx];
        }

        int take=nums[idx]+func(nums,idx+2,dp);
        int not_take=func(nums,idx+1,dp);

        return dp[idx]=Math.max(take,not_take);

    }
}
