class Solution {
    public int lengthOfLongestSubstring(String s) {
        int freq[]=new int[256];
        int si=0;
        int ei=0;
        int len=0;
        while(ei<s.length()){
            freq[s.charAt(ei)]++;

            while(freq[s.charAt(ei)]>1){
                freq[s.charAt(si)]--;
                si++;
            }

            len=Math.max(len,ei-si+1);
            ei++;

        }
        return len;
    }
}
