class Solution {
    public int eraseOverlapIntervals(int[][] intervals) {
        Arrays.sort(intervals,(a,b)->Integer.compare(a[0],b[0]));
        int ans=0;
        int val=intervals[0][1];
        for(int i=1;i<intervals.length;i++){
            if(intervals[i][0]<val){
                ans++;
                val=Math.min(val,intervals[i][1]);
            }
            else{
                val=intervals[i][1];
            }
        }
        return ans;


        
    }
}
